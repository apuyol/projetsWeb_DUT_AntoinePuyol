{"ast":null,"code":"import PopupBubbles from \"./Popup/PopupBubbles.vue\";\nimport PopupEditBubbles from \"./Popup/PopupEditBubbles.vue\";\nlet groupId = 0;\nexport default {\n  components: {\n    PopupBubbles,\n    PopupEditBubbles\n  },\n\n  data() {\n    return {\n      popupOpen: false,\n      popupEditOpen: false,\n      selectedGroup: null,\n      selectedSphere: null,\n      groups: [{\n        id: groupId++,\n        name: \"groupe 1\",\n        spheres: [{\n          id: 0,\n          name: \"Atelier\"\n        }, {\n          id: 1,\n          name: \"1er étage\"\n        }, {\n          id: 2,\n          name: \"2ème étage\"\n        }]\n      }]\n    };\n  },\n\n  methods: {\n    openOrClosePopup(group) {\n      this.popupOpen = !this.popupOpen;\n      this.selectedGroup = group;\n    },\n\n    openOrCloseEditPopup(group, sphere) {\n      this.popupOpen = !this.popupOpen;\n      this.selectedGroup = group;\n      this.selectedSphere = sphere;\n    },\n\n    addSphere(group, n) {\n      this.groups[group.id].spheres.push({\n        id: this.groups[group.id].spheres.length,\n        name: n\n      });\n    },\n\n    deleteSphere(group, sphere) {\n      this.groups[group.id].spheres = this.groups[group.id].spheres.filter(s => s !== sphere);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkFA,OAAOA,YAAP,MAAyB,0BAAzB;AACA,OAAOC,gBAAP,MAA6B,8BAA7B;AAEA,IAAIC,OAAM,GAAI,CAAd;AAEA,eAAe;AACbC,YAAU,EAAE;AAAEH,gBAAF;AAAgBC;AAAhB,GADC;;AAEbG,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE,KADN;AAELC,mBAAa,EAAE,KAFV;AAGLC,mBAAa,EAAE,IAHV;AAILC,oBAAc,EAAE,IAJX;AAKLC,YAAM,EAAE,CACN;AACEC,UAAE,EAAER,OAAO,EADb;AAEES,YAAI,EAAE,UAFR;AAGEC,eAAO,EAAE,CACP;AAAEF,YAAE,EAAE,CAAN;AAASC,cAAI,EAAE;AAAf,SADO,EAEP;AAAED,YAAE,EAAE,CAAN;AAASC,cAAI,EAAE;AAAf,SAFO,EAGP;AAAED,YAAE,EAAE,CAAN;AAASC,cAAI,EAAE;AAAf,SAHO;AAHX,OADM;AALH,KAAP;AAiBD,GApBY;;AAqBbE,SAAO,EAAE;AACPC,oBAAgB,CAACC,KAAD,EAAQ;AACtB,WAAKV,SAAL,GAAiB,CAAC,KAAKA,SAAvB;AACA,WAAKE,aAAL,GAAqBQ,KAArB;AACD,KAJM;;AAKPC,wBAAoB,CAACD,KAAD,