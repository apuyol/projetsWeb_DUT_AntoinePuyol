{"ast":null,"code":"import image from \"./ressources/planIUT.png\";\nimport { fabric } from \"fabric\";\nexport default {\n  mounted() {\n    var canvas = new fabric.Canvas(\"c\");\n    canvas.setDimensions({\n      width: 800,\n      height: 800\n    });\n    fabric.Image.fromURL(image, function (oImg) {\n      canvas.add(oImg);\n    }); // Zoom du site de fabric\n\n    canvas.on(\"mouse:wheel\", function (opt) {\n      var delta = opt.e.deltaY;\n      var zoom = canvas.getZoom();\n      zoom *= 0.999 ** delta;\n      if (zoom > 20) zoom = 20;\n      if (zoom < 0.01) zoom = 0.01;\n      canvas.setZoom(zoom);\n      opt.e.preventDefault();\n      opt.e.stopPropagation();\n    });\n    canvas.width = 500;\n  },\n\n  methods: {\n    getPos(image) {\n      console.log(this.$refs.image.getBoundingClientRect().left);\n      console.log(this.$refs.image.getBoundingClientRect().top);\n    },\n\n    test() {\n      console.log(\"test\");\n    }\n\n  },\n\n  data() {\n    return {\n      image: image\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAcA,OAAOA,KAAP,MAAkB,0BAAlB;AACA,SAASC,MAAT,QAAuB,QAAvB;AAEA,eAAe;AACbC,SAAO,GAAG;AACR,QAAIC,MAAK,GAAI,IAAIF,MAAM,CAACG,MAAX,CAAkB,GAAlB,CAAb;AACAD,UAAM,CAACE,aAAP,CAAqB;AAAEC,WAAK,EAAE,GAAT;AAAcC,YAAM,EAAE;AAAtB,KAArB;AACAN,UAAM,CAACO,KAAP,CAAaC,OAAb,CAAqBT,KAArB,EAA4B,UAAUU,IAAV,EAAgB;AAC1CP,YAAM,CAACQ,GAAP,CAAWD,IAAX;AACD,KAFD,EAHQ,CAMR;;AACAP,UAAM,CAACS,EAAP,CAAU,aAAV,EAAyB,UAAUC,GAAV,EAAe;AACtC,UAAIC,KAAI,GAAID,GAAG,CAACE,CAAJ,CAAMC,MAAlB;AACA,UAAIC,IAAG,GAAId,MAAM,CAACe,OAAP,EAAX;AACAD,UAAG,IAAK,SAASH,KAAjB;AACA,UAAIG,IAAG,GAAI,EAAX,EAAeA,IAAG,GAAI,EAAP;AACf,UAAIA,IAAG,GAAI,IAAX,EAAiBA,IAAG,GAAI,IAAP;AACjBd,YAAM,CAACgB,OAAP,CAAeF,IAAf;AACAJ,SAAG,CAACE,CAAJ,CAAMK,cAAN;AACAP,SAAG,CAACE,CAAJ,CAAMM,eAAN;AACD,KATD;AAUAlB,UAAM,CAACG,KAAP,GAAe,GAAf;AACD,GAnBY;;AAoBbgB,SAAO,EAAE;AACPC,UAAM,CAACvB,KAAD,EAAQ;AACZwB,aAAO,CAACC,GAAR,CAAY,KAAKC,KAAL,CAAW1B,KAAX,CAAiB2B,qBAAjB,GAAyCC,IAArD;AACAJ,aAAO,CAACC,GAAR,CAAY,KAAKC,KAAL,CAAW1B,K