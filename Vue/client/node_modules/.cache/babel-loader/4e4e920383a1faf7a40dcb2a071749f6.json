e = scene;\n    },\n    openOrCloseEditPopup(group, sphere) {\n      this.popupEditOpen = !this.popupEditOpen;\n      this.selectedGroup = group;\n      this.selectedSphere = sphere;\n    },\n    openOrCloseScenesPopup() {\n      this.popupScenesOpen = !this.popupScenesOpen;\n    },\n    addSceneComponents(scene, n) {\n      this.scenes[scene.id].sceneComponents.push({\n        id: this.scenes[scene.id].sceneComponents.length,\n        name: n,\n      });\n    },\n    deleteScene(scene, sceneComponent) {\n      this.scenes[scene.id].sceneComponents = this.scenes[\n        scene.id\n      ].sceneComponents.filter((s) => s !== sceneComponent);\n    },\n    editSphere(group, sphere, name) {\n      this.groups[group.id].spheres[sphere.id].name = name;\n    },\n    addScene(name) {\n      this.scenes.push({ id: sceneId++, name: name, sceneComponents: [] });\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}