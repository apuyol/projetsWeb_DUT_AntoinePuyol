{"ast":null,"code":"import image from \"./ressources/planIUT.png\";\nimport { fabric } from \"fabric\";\nlet canva = null;\nexport default {\n  mounted() {\n    var canvas = new fabric.Canvas(\"c\");\n    fabric.Image.fromURL(image, function (oImg) {\n      canvas.setDimensions({\n        width: oImg.width,\n        height: oImg.height\n      });\n      canvas.add(oImg);\n    }); // Zoom du site de fabric\n\n    canvas.on(\"mouse:wheel\", function (opt) {\n      var delta = opt.e.deltaY;\n      var zoom = canvas.getZoom();\n      zoom *= 0.999 ** delta;\n      if (zoom > 20) zoom = 20;\n      if (zoom < 0.01) zoom = 0.01;\n      canvas.setZoom(zoom);\n      opt.e.preventDefault();\n      opt.e.stopPropagation();\n    });\n    canvas.width = 500;\n    canva = canvas;\n  },\n\n  methods: {\n    getPos(image) {\n      console.log(this.$refs.image.getBoundingClientRect().left);\n      console.log(this.$refs.image.getBoundingClientRect().top);\n    },\n\n    test() {\n      console.log(canva.item(0));\n    }\n\n  },\n\n  data() {\n    return {\n      image: image\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAeA,OAAOA,KAAP,MAAkB,0BAAlB;AACA,SAASC,MAAT,QAAuB,QAAvB;AAEA,IAAIC,KAAI,GAAI,IAAZ;AACA,eAAe;AACbC,SAAO,GAAG;AACR,QAAIC,MAAK,GAAI,IAAIH,MAAM,CAACI,MAAX,CAAkB,GAAlB,CAAb;AACAJ,UAAM,CAACK,KAAP,CAAaC,OAAb,CAAqBP,KAArB,EAA4B,UAAUQ,IAAV,EAAgB;AAC1CJ,YAAM,CAACK,aAAP,CAAqB;AAAEC,aAAK,EAAEF,IAAI,CAACE,KAAd;AAAqBC,cAAM,EAAEH,IAAI,CAACG;AAAlC,OAArB;AACAP,YAAM,CAACQ,GAAP,CAAWJ,IAAX;AACD,KAHD,EAFQ,CAMR;;AACAJ,UAAM,CAACS,EAAP,CAAU,aAAV,EAAyB,UAAUC,GAAV,EAAe;AACtC,UAAIC,KAAI,GAAID,GAAG,CAACE,CAAJ,CAAMC,MAAlB;AACA,UAAIC,IAAG,GAAId,MAAM,CAACe,OAAP,EAAX;AACAD,UAAG,IAAK,SAASH,KAAjB;AACA,UAAIG,IAAG,GAAI,EAAX,EAAeA,IAAG,GAAI,EAAP;AACf,UAAIA,IAAG,GAAI,IAAX,EAAiBA,IAAG,GAAI,IAAP;AACjBd,YAAM,CAACgB,OAAP,CAAeF,IAAf;AACAJ,SAAG,CAACE,CAAJ,CAAMK,cAAN;AACAP,SAAG,CAACE,CAAJ,CAAMM,eAAN;AACD,KATD;AAUAlB,UAAM,CAACM,KAAP,GAAe,GAAf;AACAR,SAAI,GAAIE,MAAR;AACD,GApBY;;AAqBbmB,SAAO,EAAE;AACPC,UAAM,C