{"ast":null,"code":"import PopupDoors from \"./Popup/PopupDoors.vue\";\nimport PopudEditDoors from \"./Popup/PopupEditDoors.vue\";\nlet id = 0;\nexport default {\n  components: {\n    PopupDoors\n  },\n\n  data() {\n    return {\n      name: \"\",\n      popupOpen: false,\n      editPopupOpen: false,\n      selected: null,\n      doors: [{\n        id: id++,\n        name: \"Porte 1\"\n      }, {\n        id: id++,\n        name: \"Porte 2\"\n      }, {\n        id: id++,\n        name: \"Porte 3\"\n      }]\n    };\n  },\n\n  methods: {\n    addDoor(n) {\n      this.doors.push({\n        id: id++,\n        name: n\n      });\n      this.name = \"\";\n    },\n\n    removeDoor(door) {\n      this.doors = this.doors.filter(t => t !== door);\n    },\n\n    openOrClosePopup() {\n      this.popupOpen = !this.popupOpen;\n    },\n\n    openOrCloseEditPopup(object) {\n      this.editPopupOpen = !this.editPopupOpen;\n      this.selected = object;\n    },\n\n    editDoor(door, n) {\n      this.doors[door.id].name = n;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqDA,OAAOA,UAAP,MAAuB,wBAAvB;AACA,OAAOC,cAAP,MAA2B,4BAA3B;AAEA,IAAIC,EAAC,GAAI,CAAT;AAEA,eAAe;AACbC,YAAU,EAAE;AAAEH;AAAF,GADC;;AAGbI,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,EADD;AAELC,eAAS,EAAE,KAFN;AAGLC,mBAAa,EAAE,KAHV;AAILC,cAAQ,EAAE,IAJL;AAKLC,WAAK,EAAE,CACL;AAAEP,UAAE,EAAEA,EAAE,EAAR;AAAYG,YAAI,EAAE;AAAlB,OADK,EAEL;AAAEH,UAAE,EAAEA,EAAE,EAAR;AAAYG,YAAI,EAAE;AAAlB,OAFK,EAGL;AAAEH,UAAE,EAAEA,EAAE,EAAR;AAAYG,YAAI,EAAE;AAAlB,OAHK;AALF,KAAP;AAWD,GAfY;;AAgBbK,SAAO,EAAE;AACPC,WAAO,CAACC,CAAD,EAAI;AACT,WAAKH,KAAL,CAAWI,IAAX,CAAgB;AAAEX,UAAE,EAAEA,EAAE,EAAR;AAAYG,YAAI,EAAEO;AAAlB,OAAhB;AACA,WAAKP,IAAL,GAAY,EAAZ;AACD,KAJM;;AAKPS,cAAU,CAACC,IAAD,EAAO;AACf,WAAKN,KAAL,GAAa,KAAKA,KAAL,CAAWO,MAAX,CAAmBC,CAAD,IAAOA,MAAMF,IAA/B,CAAb;AACD,KAPM;;AAQPG,oBAAgB,GAAG;AACjB,WAAKZ,SAAL,GAAiB,CAAC,KAAKA,SAAvB;AACD,KAVM;;AAWPa,wBAAoB,CAACC,MAAD,EAAS;AAC3B,WAAKb,aAAL,GAAqB,CAAC,KAAKA,aAA3B;AACA,WAAKC,QAAL,GAAgBY,MAAhB;AACD,KAdM;;AAePC,YAAQ,CAACN,IAAD,EAAOH,CAAP,EAAU;AAChB,WAAKH,KAAL,CAAWM,IAAI,CAACb,EAAhB,EAAoBG,IAApB,GAA2BO,CAA3B;AACD;;AAjBM;AAhBI,CAAf","names":["PopupDoors","PopudEditDoors","id","components","data","name","popupOpen","editPopupOpen","selected","doors","methods","addDoor","n","push","removeDoor","door","filter","t","openOrClosePopup","openOrCloseEditPopup","object","editDoor"],"sourceRoot":"","sources":["D:\\Stage\\App\\xr-place-application\\client\\src\\components\\MyDoors.vue"],"sourcesContent":["<template>\n  <PopupDoors v-if=\"popupOpen\" :status=\"openOrClosePopup\" :add=\"addDoor\" />\n  <PopudEditDoors\n    v-if=\"editPopupOpen\"\n    :status=\"openOrClosePopup\"\n    :edit=\"editDoor\"\n    :item=\"selected\"\n  />\n  <ul>\n    <li v-for=\"door in doors\" :key=\"door.id\" class=\"scene\">\n      <v-btn\n        icon=\"mdi-cog\"\n        color=\"#607D8B\"\n        size=\"25\"\n        variant=\"outlined\"\n        @click=\"openOrCloseEditPopup(door)\"\n      ></v-btn>\n      <p>{{ door.name }}</p>\n      <v-btn\n        icon=\"mdi-close\"\n        color=\"#EF5350\"\n        size=\"25\"\n        variant=\"outlined\"\n        @click=\"removeDoor(door)\"\n      ></v-btn>\n    </li>\n  </ul>\n  <div class=\"scene\" style=\"width: 100%\">\n    <v-btn\n      icon=\"mdi-plus\"\n      color=\"#81C784\"\n      class=\"add\"\n      size=\"25\"\n      variant=\"outlined\"\n      @click=\"openOrClosePopup\"\n    ></v-btn>\n  </div>\n</template>\n\n<style scoped>\n.scene {\n  display: flex;\n  justify-content: space-between;\n  margin: 5%;\n}\n\n.add {\n  display: flex;\n  margin-left: auto;\n}\n</style>\n\n<script>\nimport PopupDoors from \"./Popup/PopupDoors.vue\";\nimport PopudEditDoors from \"./Popup/PopupEditDoors.vue\";\n\nlet id = 0;\n\nexport default {\n  components: { PopupDoors },\n\n  data() {\n    return {\n      name: \"\",\n      popupOpen: false,\n      editPopupOpen: false,\n      selected: null,\n      doors: [\n        { id: id++, name: \"Porte 1\" },\n        { id: id++, name: \"Porte 2\" },\n        { id: id++, name: \"Porte 3\" },\n      ],\n    };\n  },\n  methods: {\n    addDoor(n) {\n      this.doors.push({ id: id++, name: n });\n      this.name = \"\";\n    },\n    removeDoor(door) {\n      this.doors = this.doors.filter((t) => t !== door);\n    },\n    openOrClosePopup() {\n      this.popupOpen = !this.popupOpen;\n    },\n    openOrCloseEditPopup(object) {\n      this.editPopupOpen = !this.editPopupOpen;\n      this.selected = object;\n    },\n    editDoor(door, n) {\n      this.doors[door.id].name = n;\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}