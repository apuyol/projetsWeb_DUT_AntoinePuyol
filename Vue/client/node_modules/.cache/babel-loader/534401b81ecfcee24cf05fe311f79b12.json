{"ast":null,"code":"import image from \"./ressources/planIUT.png\";\nimport { fabric } from \"fabric\";\nlet canva = null;\nexport default {\n  mounted() {\n    var canvas = new fabric.Canvas(\"c\");\n    fabric.Image.fromURL(image, function (oImg) {\n      canvas.setDimensions({\n        width: oImg.width,\n        height: oImg.height\n      });\n      canvas.on(\"mouse:down\", function (e) {\n        var pointer = this.getPointer(e);\n        var posX = oImg.width - e.pointer.x;\n        var posY = e.pointer.y;\n        console.log(posX); // Log to console\n      });\n      canvas.add(oImg);\n    }); // Zoom du site de fabric\n\n    canvas.on(\"mouse:wheel\", function (opt) {\n      var delta = opt.e.deltaY;\n      var zoom = canvas.getZoom();\n      zoom *= 0.999 ** delta;\n      if (zoom > 20) zoom = 20;\n      if (zoom < 0.01) zoom = 0.01;\n      canvas.setZoom(zoom);\n      opt.e.preventDefault();\n      opt.e.stopPropagation();\n    });\n    canvas.width = 500;\n    canva = canvas;\n  },\n\n  methods: {\n    getPos(image) {\n      console.log(this.$refs.image.getBoundingClientRect().left);\n      console.log(this.$refs.image.getBoundingClientRect().top);\n    },\n\n    test() {\n      console.log(canva.item(0));\n    },\n\n    getMouseCoords(event) {\n      var pointer = canva.getPointer(event.e);\n      var posX = pointer.x;\n      var posY = pointer.y;\n      console.log(posX + \", \" + posY); // Log to console\n    }\n\n  },\n\n  data() {\n    return {\n      image: image\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAeA,OAAOA,KAAP,MAAkB,0BAAlB;AACA,SAASC,MAAT,QAAuB,QAAvB;AAEA,IAAIC,KAAI,GAAI,IAAZ;AACA,eAAe;AACbC,SAAO,GAAG;AACR,QAAIC,MAAK,GAAI,IAAIH,MAAM,CAACI,MAAX,CAAkB,GAAlB,CAAb;AAEAJ,UAAM,CAACK,KAAP,CAAaC,OAAb,CAAqBP,KAArB,EAA4B,UAAUQ,IAAV,EAAgB;AAC1CJ,YAAM,CAACK,aAAP,CAAqB;AAAEC,aAAK,EAAEF,IAAI,CAACE,KAAd;AAAqBC,cAAM,EAAEH,IAAI,CAACG;AAAlC,OAArB;AACAP,YAAM,CAACQ,EAAP,CAAU,YAAV,EAAwB,UAAUC,CAAV,EAAa;AACnC,YAAIC,OAAM,GAAI,KAAKC,UAAL,CAAgBF,CAAhB,C