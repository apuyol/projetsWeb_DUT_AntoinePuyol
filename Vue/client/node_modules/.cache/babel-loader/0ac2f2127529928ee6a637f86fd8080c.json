up, sphere) {\n      this.popupEditOpen = !this.popupEditOpen;\n      this.selectedGroup = group;\n      this.selectedSphere = sphere;\n    },\n    openOrCloseGroupsPopup() {\n      this.popupGroupsOpen = !this.popupGroupsOpen;\n    },\n    sendToCanva(sphere) {\n      this.addCanva(sphere);\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}