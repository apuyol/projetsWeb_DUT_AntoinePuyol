{"ast":null,"code":"import image from \"./ressources/planIUT.png\";\nimport { fabric } from \"fabric\";\nexport default {\n  mounted() {\n    var canvas = new fabric.Canvas(\"c\");\n    fabric.Image.fromURL(image, function (oImg) {\n      canvas.add(oImg);\n    }); // Zoom du site de fabric\n\n    canvas.on(\"mouse:wheel\", function (opt) {\n      var delta = opt.e.deltaY;\n      var zoom = canvas.getZoom();\n      zoom *= 0.999 ** delta;\n      if (zoom > 20) zoom = 20;\n      if (zoom < 0.01) zoom = 0.01;\n      canvas.setZoom(zoom);\n      opt.e.preventDefault();\n      opt.e.stopPropagation();\n    });\n  },\n\n  methods: {\n    getPos(image) {\n      console.log(this.$refs.image.getBoundingClientRect().left);\n      console.log(this.$refs.image.getBoundingClientRect().top);\n    },\n\n    test() {\n      console.log(\"test\");\n    }\n\n  },\n\n  data() {\n    return {\n      image: image\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAcA,OAAOA,KAAP,MAAkB,0BAAlB;AACA,SAASC,MAAT,QAAuB,QAAvB;AAEA,eAAe;AACbC,SAAO,GAAG;AACR,QAAIC,MAAK,GAAI,IAAIF,MAAM,CAACG,MAAX,CAAkB,GAAlB,CAAb;AACAH,UAAM,CAACI,KAAP,CAAaC,OAAb,CAAqBN,KAArB,EAA4B,UAAUO,IAAV,EAAgB;AAC1CJ,YAAM,CAACK,GAAP,CAAWD,IAAX;AACD,KAFD,EAFQ,CAKR;;AACAJ,UAAM,CAACM,EAAP,CAAU,aAAV,EAAyB,UAAUC,GAAV,EAAe;AACtC,UAAIC,KAAI,GAAID,GAAG,CAACE,CAAJ,CAAMC,MAAlB;AACA,UAAIC,IAAG,GAAIX,MAAM,CAACY,OAAP,EAAX;AACAD,UAAG,IAAK,SAASH,KAAjB;AACA,UAAIG,IAAG,GAAI,EAAX,EAAeA,IAAG,GAAI,EAAP;AACf,UAAIA,IAAG,GAAI,IAAX,EAAiBA,IAAG,GAAI,IAAP;AACjBX,YAAM,CAACa,OAAP,CAAeF,IAAf;AACAJ,SAAG,CAACE,CAAJ,CAAMK,cAAN;AACAP,SAAG,CAACE,CAAJ,CAAMM,eAAN;AACD,KATD;AAUD,GAjBY;;AAkBbC,SAAO,EAAE;AACPC,UAAM,CAACpB,KAAD,EAAQ;AACZqB,aAAO,CAACC,GAAR,CAAY,KAAKC,KAAL,CAAWvB,KAAX,CAAiBwB,qBAAjB,GAAyCC,IAArD;AACAJ,aAAO,CAACC,GAAR,CAAY,KAAKC,KAAL,CAAWvB,KAAX,CAAiBwB,qBAAjB,GAAyCE,GAArD;AACD,KAJM;;AAKPC,QAAI,GAAG;AACLN,aAAO,CAACC,GAAR,CAAY,MAAZ;AACD;;AAPM,GAlBI;;AA2BbM,MAAI,GAAG;AACL,WAAO;AACL5B,WAAK,EAAEA;AADF,KAAP;AAGD;;AA/BY,CAAf","names":["image","fabric","mounted","canvas","Canvas","Image","fromURL","oImg","add","on","opt","delta","e","deltaY","zoom","getZoom","setZoom","preventDefault","stopPropagation","methods","getPos","console","log","$refs","getBoundingClientRect","left","top","test","data"],"sourceRoot":"","sources":["D:\\Stage\\App\\xr-place-application\\client\\src\\components\\RightPanel.vue"],"sourcesContent":["<template>\n  <!--\n  <img\n    ref=\"image\"\n    :src=\"image\"\n    style=\"object-fit: contain; width: 100%; height: 100%\"\n    class=\"zoomable\"\n    @click=\"getPos(image)\"\n    @mousewheel=\"test\"\n  /> -->\n  <canvas id=\"c\"></canvas>\n</template>\n\n<script>\nimport image from \"./ressources/planIUT.png\";\nimport { fabric } from \"fabric\";\n\nexport default {\n  mounted() {\n    var canvas = new fabric.Canvas(\"c\");\n    fabric.Image.fromURL(image, function (oImg) {\n      canvas.add(oImg);\n    });\n    // Zoom du site de fabric\n    canvas.on(\"mouse:wheel\", function (opt) {\n      var delta = opt.e.deltaY;\n      var zoom = canvas.getZoom();\n      zoom *= 0.999 ** delta;\n      if (zoom > 20) zoom = 20;\n      if (zoom < 0.01) zoom = 0.01;\n      canvas.setZoom(zoom);\n      opt.e.preventDefault();\n      opt.e.stopPropagation();\n    });\n  },\n  methods: {\n    getPos(image) {\n      console.log(this.$refs.image.getBoundingClientRect().left);\n      console.log(this.$refs.image.getBoundingClientRect().top);\n    },\n    test() {\n      console.log(\"test\");\n    },\n  },\n  data() {\n    return {\n      image: image,\n    };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}