lse if (this.typeObject == \"text\") {\n        this.editText(this.object, this.name, this.text);\n      } else if (this.typeObject == \"image\") {\n        this.editImage(this.object, this.name, this.file[0]);\n      }\n\n      this.status();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkFA,SAASA,MAAT,QAAuB,QAAvB;AAEA,eAAe;AACbC,OAAK,EAAE;AACLC,UAAM,EAAEC,QADH;AAELC,UAAM,EAAEC,MAFH;AAGLC,cAAU,EAAEC,MAHP;AAILC,cAAU,EAAEL,QAJP;AAKLM,YAAQ,EAAEN,QALL;AAMLO,aAAS,EAAEP,QANN;AAOLQ,mBAAe,EAAER;AAPZ,GADM;;AAUbS,SAAO,GAAG;AACR,QAAI,KAAKN,UAAL,IAAmB,QAAvB,EAAiC;AAC/B,UAAIO,MAAK,GAAI,IAAIb,MAAM,CAACc,MAAX,CAAkB,SAAlB,CAAb;AACAD,YAAM,CAACE,aAAP,CAAqB;AACnBC,aAAK,EAAEC,QAAQ,CAACC,cAAT,CAAwB,aAAxB,EAAuCC,WAAvC,GAAqD,EADzC;AAEnBC,cAAM,EAAE;AAFW,OAArB;AAKA,UAAIC,KAAI,GAAI,IAAIrB,MAAM,CAACsB,MAAX,CAAkB;AAC5BC,cAAM,EAAE,CADoB;AAE5BC,WAAG,EAAE,KAAKC,CAFkB;AAG5BC,YAAI,EAAE,KAAKC,CAHiB;AAI5BC,YAAI,EAAE;AAJsB,OAAlB,CAAZ;AAMAf,YAAM,CAACgB,YAAP,CAAoBR,KAApB;AAEA,UAAIS,MAAK,GAAI,IAAI9B,MAAM,CAACsB,MAAX,CAAkB;AAC7BC,cAAM,EAAE,KAAKQ,YADgB;AAE7BP,WAAG,EAAEX,MAAM,CAACO,MAAP,GAAgB,CAAhB,GAAoB,KAAKW,YAFD;AAG7BL,YAAI,EAAEb,MAAM,CAACG,KAAP,GAAe,CAAf,GAAmB,KAAKe,YAHD;AAI7BH,YAAI,EAAE,OAJuB;AAK7BI,cAAM,EAAE,OALqB;AAM7BC,kBAAU,EAAE;AANiB,OAAlB,CAAb;AAQApB,YAAM,CAACqB,UAAP,CAAkBJ,MAAlB;AAEA,WAAKK,KAAL,GAAatB,MAAb;AACF;AACD,GAtCY;;AAuCbuB,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,KAAKjC,MAAL,CAAYiC,IADb;AAELC,UAAI,EAAEC,SAAQ,IAAK,KAAKnC,MAAL,CAAYoC,GAF1B;AAGLC,UAAI,EAAEF,SAAQ,IAAK,CAAC,KAAKnC,MAAL,CAAYqC,IAAb,CAHd;AAILV,kBAAY,EAAEQ,SAAQ,IAAK,KAAKnC,MAAL,CAAYmB,MAJlC;AAKLI,OAAC,EAAEY,SAAQ,IAAK,KAAKnC,MAAL,CAAYuB,CALvB;AAMLF,OAAC,EAAEc,SAAQ,IAAK,KAAKnC,MAAL,CAAYqB;AANvB,KAAP;AAQD,GAhDY;;AAiDbiB,OAAK,EAAE;AACLX,gBAAY,GAAG;AACb,WAAKI,KAAL,CAAWQ,MAAX,CAAkB,KAAKR,KAAL,CAAWS,UAAX,GAAwB,CAAxB,CAAlB;AACA,UAAId,MAAK,GAAI,IAAI9B,MAAM,CAACsB,MAAX,CAAkB;AAC7BC,cAAM,EAAE,KAAKQ,YADgB;AAE7BP,WAAG,EAAE,KAAKW,KAAL,CAAWf,MAAX,GAAoB,CAApB,GAAwB,KAAKW,YAFL;AAG7BL,YAAI,EAAE,KAAKS,KAAL,CAAWnB,KAAX,GAAmB,CAAnB,GAAuB,KAAKe,YAH